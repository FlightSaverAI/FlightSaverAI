{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "flight-saver": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/flight-saver",
      "sourceRoot": "projects/flight-saver/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-architects/native-federation:build",
          "options": {},
          "configurations": {
            "production": {
              "target": "flight-saver:esbuild:production"
            },
            "development": {
              "target": "flight-saver:esbuild:development",
              "dev": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "flight-saver:serve-original:development",
            "rebuildDelay": 0,
            "dev": true,
            "port": 0
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "flight-saver:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/flight-saver/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/flight-saver/src/favicon.ico",
              "projects/flight-saver/src/assets",
              "projects/flight-saver/src/manifest.webmanifest"
            ],
            "styles": ["projects/flight-saver/src/styles.scss"],
            "scripts": []
          }
        },
        "esbuild": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/flight-saver",
            "index": "projects/flight-saver/src/index.html",
            "browser": "projects/flight-saver/src/main.ts",
            "polyfills": ["zone.js", "es-module-shims"],
            "tsConfig": "projects/flight-saver/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/flight-saver/src/favicon.ico",
              "projects/flight-saver/src/assets",
              "projects/flight-saver/src/manifest.webmanifest"
            ],
            "styles": ["projects/flight-saver/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "projects/flight-saver/ngsw-config.json"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-original": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "flight-saver:esbuild:production"
            },
            "development": {
              "buildTarget": "flight-saver:esbuild:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4201
          }
        }
      }
    },
