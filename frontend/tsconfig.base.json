      "@flight-saver/user-profile/ui": ["libs/user-profile/ui/src/index.ts"],
      "@flight-saver/user-profile/utils": ["libs/user-profile/utils/src/index.ts"],
      "@global/*": ["apps/global/*"],
      "@shared/data-access": ["libs/shared/data-access/src/index.ts"],
      "@shared/ui": ["libs/shared/ui/src/index.ts"],
      "@shared/ui-components": ["libs/shared/ui-components/src/index.ts"]
    }
